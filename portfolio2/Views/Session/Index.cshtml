@model IEnumerable<portfolio2.Models.SessionViewModel>

@{
    ViewData["Title"] = "Session";
    if (Context.Session.GetInt32("StudentID") != null)
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_SignInLayout.cshtml";
    }
}

<h4 class="PageTitle">View All Session</h4>

<partial name="~/Views/Session/_ViewSession.cshtml" model="@Model" />
